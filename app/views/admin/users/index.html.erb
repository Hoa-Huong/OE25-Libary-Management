<% provide :title, t("user_manage") %>
<h2 class = "title"><%= t "user_manage" %></h2>
<h5>Total: <%= User.all.size %></h5>
<table class="table table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th scope="col"><%= t "stt" %></th>
      <th scope="col"><%= t "user_name" %></th>
      <th scope="col"><%= t "birth" %></th>
      <th scope="col"><%= t "email" %></th>
      <th scope="col"><%= t "phone" %></th>
      <th scope="col"><%= t "control" %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @users %>
  </tbody>
</table>
<%= paginate @users %>
<div id="modal-window" class="modal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content"></div>
  </div>
</div>

