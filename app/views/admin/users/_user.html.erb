<tr>
  <td><%= user.name %></td>
  <td><%= user.birth %></td>
  <td><%= user.email %> </td>
  <td><%= user.phone %></td>
  <td>
    <span>
      <%= link_to admin_user_path(user), {remote: true, data:{toggle: "modal"}, data: {target: "#modal-window"}, class: "book-btn"} do %>
        <i class="fa fa-eye"></i>
      <% end %>
    </span>
    <span>
      <%= link_to  edit_admin_user_path(user), {remote: true, data:{toggle: "modal"}, data: {target: "#modal-window"}, class: "book-btn"} do %>
        <i class="fa fa-edit"></i>
      <% end %>
    </span>
    <span>
      <%= link_to admin_user_path(user), method: :delete, data: { confirm: t("sure") }, class: "book-btn" do %>
        <i class="fa fa-trash"></i>
      <% end %>
    </span>
  </td>
</tr>
